{
       "headers": [
         {
           "source": "/(.*)",
           "headers": [
             {
               "key": "Content-Security-Policy",
               "value": "frame-ancestors 'self' https://verticedigital.com.ar;"
             },
            {
              "key": "X-Frame-Options",
              "value": "ALLOWALL"
            }
          ]
        }
      ]
}